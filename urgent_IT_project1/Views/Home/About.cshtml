
<style>
    /* Existing styles */

    .team-member {
        transition: transform 0.3s ease-in-out;
    }

        .team-member img {
            width: 100%;
            transition: transform 0.3s ease-in-out;
        }

        .team-member:hover {
            transform: translateY(-10px); /* Move up on hover */
        }

            .team-member:hover img {
                transform: scale(1.05); /* Slight zoom effect on hover */
            }

    .animate-left {
        transform: translateX(-100%);
        transition: transform 3s ease-in-out;
    }

    .animate-right {
        transform: translateX(100%);
        transition: transform 3s ease-in-out;
    }

    .animate-up {
        transform: translateY(100%);
        transition: transform 3s ease-in-out;
    }

    .setimg {
        margin: 0;
        font-family: Arial, sans-serif;
        background-image: url('/index/bgimg.png');
        background-size: cover;
        background-attachment: fixed;
        color: white;
    }

    .round {
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100vh; /* Center the image vertically */
        border: 5px solid #000; /* Change border size and color as needed */
        border-radius: 50%;
        width: 200px; /* Adjust the size as needed */
        height: 200px; /* Adjust the size as needed */
        object-fit: cover; /* Ensure the image covers the element while maintaining its aspect ratio */
    }

    .contenttt {
        background-color: rgba(0, 0, 1, 0.6); /* Black background with 60% opacity */
        padding: 20px;
        margin: 20px;
        border-radius: 10px;
        background-attachment: fixed;
    }

        /* Hover effect for .contenttt images */
        .contenttt img {
            transition: transform 0.3s ease-in-out;
        }

        /* Hover effect for .contenttt text */
        .contenttt h3, .contenttt p {
            transition: color 0.3s ease-in-out, transform 0.3s ease-in-out;
        }

        /* Apply hover effect to .contenttt */
        .contenttt:hover img {
            transform: scale(1.1); /* Slight zoom effect on hover for images */
        }

        .contenttt:hover h3, .contenttt:hover p {
            color: #f8f9fa; /* Change text color on hover */
            transform: translateY(-5px); /* Slight move up effect on hover for text */
        }

    /* Other existing styles */
</style>


