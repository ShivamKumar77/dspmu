    .setimg {
        margin: 0;
        background-image: url('/index/bgimg.png');
        background-size: cover;
        background-attachment: fixed;
    }

    /* Image and text hover effect for team members */
    .team-members img {
        transition: transform 0.3s ease-in-out;
        transform: scale(1);
    }

    .team-members .member-info {
        transition: transform 0.3s ease-in-out, color 0.3s ease-in-out;
        transform: translateY(0);
    }

    /* Hover effect: scaling image and moving text */
    .team-members:hover img {
        transform: scale(1.1);
    }

    .team-members:hover .member-info {
        transform: translateY(-10px);
        color: #f8f9fa; /* Change text color on hover */
    }

    /* Animation for page load */
    .team-members {
        opacity: 0;
        transform: translateY(50px);
        transition: opacity 0.5s ease-in-out, transform 0.5s ease-in-out;
    }

        .team-members.animated {
            opacity: 1;
            transform: translateY(0);
        }
</style>

    
