<style>
    body {
        margin: 0;
        font-family: Arial, sans-serif;
        background-image: url('/slider/slider4.png');
        background-size: cover;
        background-attachment: fixed;
        color: white;
    }

    .contenttt {
        background-color: rgba(0, 0, 1, 0.6); /* Black background with 60% opacity */
        padding: 20px;
        margin: 20px;
        border-radius: 10px;
        background-attachment: fixed;
    }
    .category-item {
        padding: 15px;
        border: 1px solid #dee2e6;
        border-radius: 10px;
        margin-bottom: 15px;
        display: flex;
        align-items: center; /* Align items vertically */
        text-align: left;
        transition: background-color 0.3s ease, transform 0.3s ease;
        height: 150px; /* Reduced height */
    }

        .category-item:hover {
            background-color: #fa8072;
            transform: scale(1.05);
        }

    .category-icon {
        font-size: 30px; /* Adjusted icon size */
        color: #dc143c;
        background-color: #000000;
        border-radius: 50%;
        width: 50px; /* Adjusted width */
        height: 50px; /* Adjusted height */
        display: flex;
        align-items: center;
        justify-content: center;
        margin-right: 15px; /* Space between icon and text */
        transition: background-color 0.3s ease, color 0.3s ease;
    }

    .category-item:hover .category-icon {
        background-color: #fa8072;
        color: #ffffff;
    }

    .category-item h4 {
        font-size: 16px; /* Smaller font size for headers */
        margin-bottom: 5px; /* Adjust margin to fit smaller text */
    }

    .category-item p {
        font-size: 14px; /* Smaller font size for paragraphs */
        margin: 0; /* Remove margin for better alignment */
    }

    .equal-height {
        display: flex;
        flex-wrap: wrap;
    }

        .equal-height .col {
            display: flex;
            flex-direction: column;
        }

    .col-md-6 {
        display: flex;
        flex-direction: column;
    }

    .category-item:last-child {
        margin-bottom: 0; /* Remove margin for the last item */
    }

</style>


